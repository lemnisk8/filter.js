{
  "name": "@mg98/filter-js",
  "version": "0.4.1",
  "description": "Filter JSONs using SQL-like conditional expression syntax!",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npx tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && browserify lib/cjs/index.js -s filterjs -o lib/bundle.js && uglifyjs lib/bundle.js > lib/bundle.min.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mg98/filter.js.git"
  },
  "publishConfig": {
    "name": "@mg98/filter-js",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "sql",
    "json"
  ],
  "author": "Marcel Gregoriadis <m.gregoriadis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "babel-jest": "^27.5.1",
    "browserify": "^17.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "uglify-js": "^3.16.0"
  }
}
